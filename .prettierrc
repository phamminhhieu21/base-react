{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "importOrder": ["^(?!app)\\w", "^app", "^\\./", "^\\.\\./"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
